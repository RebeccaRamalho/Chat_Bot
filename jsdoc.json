{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "include": ["script", "index.html"],
    "includePattern": ".js$",
    "excludePattern": "(nodes_modules|docs)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },

  "opts": {
    "destination": "./jsdoc",
    "recurse": true
  }
}
